                    "hide_final_grades": "string*",
                    "license": "string*",
                    "allow_student_assignment_edits": "string*",
                    "allow_wiki_comments": "string*",
                    "allow_student_forum_attachments": "string*",
                    "open_enrollment": "string*",
                    "self_enrollment": "string*",
                    "restrict_enrollments_to_course_dates": "string*",
                    "course_format": "string*",
                    "access_restricted_by_date": "string*",
                    "time_zone": "string*",
                    "blueprint": "string*",
                    "blueprint_restrictions": "string*",
