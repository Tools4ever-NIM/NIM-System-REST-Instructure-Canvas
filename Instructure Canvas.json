                    "current_period_unposted_final_score": "string*",
                    "current_period_unposted_current_grade": "string*",
                    "current_period_unposted_final_grade": "string*"
                }
            },
            "logins": {
                "operations": {
                    "loginsList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "accounts",
                            "iterator": "id",
                            "path": "/v1/accounts/{iterator}/logins"
                        },
                        "processing_options": {
                            "output_field": null
                        }
                    }
                },
                "key": "id",
                "resources": {
                    "account_id": "string*",
                    "id": "string*",
                    "sis_user_id": "string*",
                    "unique_id": "string*",
                    "user_id": "string*",
                    "authentication_provider_id": "string*",
                    "authentication_provider_type": "string*",
                    "workflow_state": "string*",
                    "declared_user_type": "string*"
                }
            },
            "sections": {
                "operations": {
                    "sectionsList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "courses",
                            "iterator": "id",
                            "path": "/v1/courses/{iterator}/sections"
                        },
                        "processing_options": {
                            "output_field": null
                        },
                        "maxSessionCount": 20
                    }
                },
                "key": "id",
                "resources": {
                    "id": "string*",
                    "name": "string*",
                    "sis_section_id": "string*",
                    "integration_id": "string*",
                    "sis_import_id": "string*",
                    "course_id": "string*",
                    "sis_course_id": "string*",
                    "start_at": "string*",
                    "end_at": "string*",
                    "restrict_enrollments_to_section_dates": "string*",
                    "nonxlist_course_id": "string*",
                    "total_students": "string*"
                }
            },
            "roles": {
                "operations": {
                    "rolesList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "accounts",
                            "iterator": "id",
                            "path": "/v1/accounts/{iterator}/roles"
                        },
                        "processing_options": {
                            "output_field": "enrollment_terms"
                        }
                    }
                },
                "key": "id",
                "resources": {
                    "id": "string*",
                    "label": "string*",
                    "role": "string*",
                    "base_role_type": "string*",
                    "is_account_role": "string*",
                    "account": {
                        "id": "_:string*",
                        "name": "_:string*",
                        "parent_account_id": "_:string*",
                        "root_account_id": "_:string*",
                        "sis_account_id": "_:string*"
                    },
                    "workflow_state": "string*",
                    "created_at": "string*",
                    "last_updated_at": "string*"
                }
            },
            "terms": {
                "operations": {
                    "termsList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "accounts",
                            "iterator": "id",
                            "path": "/v1/accounts/{iterator}/terms"
                        },
                        "processing_options": {
                            "output_field": "enrollment_terms"
                        }
                    }
                },
                "key": "id",
                "resources": {
                    "id": "string*",
                    "name": "string*",
                    "start_at": "string*",
                    "end_at": "string*",
                    "created_at": "string*",
                    "workflow_state": "string*",
                    "grading_period_group_id": "string*",
                    "sis_term_id": "string*",
                    "overrides": {
                        "StudentEnrollment": {
                            "start_at": "_:string*",
                            "end_at": "_:string*"
                        },
                        "TeacherEnrollments": {
                            "start_at": "string*",
                            "end_at": "string*"
                        }
                    }
                }
            },
            "users": {
                "operations": {
                    "usersList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "accounts",
                            "iterator": "id",
                            "path": "/v1/accounts/{iterator}/users",
                            "base": {
                                "account_id": "{iterator}"
                            },
                            "queryParameters": {
                                "include_deleted_users": true
                            }
                        },
                        "processing_options": {
                            "output_field": null
                        }
                    },
                    "userCreate": {
                        "method": "post",
                        "call": {
                            "mode": "normal",
                            "path": "/v1/accounts/{account_id}/users",
                            "post_processing": "as_input"
                        },
                        "postAsForm": true,
                        "removeBlankFields": true,
                        "processing_options": {},
                        "body": {
                            "_nim_merge_attributes_": false,
                            "user[name]": "{name}",
                            "user[short_name]": "{short_name}",
                            "user[sortable_name]": "{sortable_name}",
                            "user[timezone]": "{time_zone}",
                            "user[locale]": "{locale}",
                            "user[terms_of_use]": "{terms_of_use}",
                            "user[skip_registration}": "{skip_registration}",
                            "pseudonym[unique_id]": "{login_id}",
                            "pseudonym[password]": "{password}",
                            "pseudonym[sis_user_id]": "{sis_user_id}",
                            "pseudonym[integration_id]": "{integration_id}",
                            "pseudonym[send_confirmation]": "{send_confirmation}",
                            "pseudonym[force_self_registration]": "{force_self_registration}",
                            "pseudonym[authentication_provider_id]": "{authentication_provider_id}",
                            "communication_channel[type]": "email",
                            "communication_channel[address]": "{email}",
                            "communication_channel[confirmation_url]": "{confirmation_url}",
                            "force_validations": "{force_validations}",
                            "enable_sis_reactivation": "{enable_sis_reactivation}",
                            "destination": "{destination}",
                            "initial_enrollment_type": "{initial_enrollment_type}",
                            "pairing_code[code]": "{pairing_code}"

                        },
                        "semantics": "create",
                        "resource_allowance_default": "optional",
                        "resource_prohibited": [
                            "last_name",
                            "first_name"

                        ],
                        "resource_mandatory": [
                            "name",
                            "login_id",
                            "email",
                            "account_id"
                        ]
                    },
                    "userUpdate": {
                        "method": "put",
                        "call": {
                            "mode": "normal",
                            "path": "/v1/users/{id}",
                            "post_processing": "as_input"
                        },
                        "postAsForm": true,
                        "removeBlankFields": true,
                        "processing_options": {},
                        "body": {
                            "_nim_merge_attributes_": false,
                            "user[name]": "{name}",
                            "user[short_name]": "{short_name}",
                            "user[sortable_name]": "{sortable_name}",
                            "user[timezone]": "{time_zone}",
                            "user[locale]": "{locale}",
                            "user[avatar][url]": "{avatar_url}",
                            "user[avatar][state]": "{avatar_state}"
                        },
                        "semantics": "update",
                        "resource_allowance_default": "optional",
                        "resource_prohibited": [
                            "last_name",
                            "first_name"

                        ],
                        "resource_mandatory": [
                            "id"
                        ]
                    },
                    "userDelete": {
                        "method": "delete",
                        "call": {
                            "mode": "normal",
                            "path": "/v1/accounts/{account_id}/users/{id}",
                            "post_processing": "as_input"
                        },
                        "semantics": "delete",
                        "resource_allowance_default": "prohibited",
                        "resource_mandatory": [
                            "id",
                            "account_id"
                        ]
                    },
                    "userTerminateSessions": {
                        "method": "delete",
                        "call": {
                            "mode": "normal",
                            "path": "/v1/users/{id}/sessions",
                            "post_processing": "as_input"
                        },
                        "resource_allowance_default": "prohibited",
                        "resource_mandatory": [
                            "id",
                            "account_id"
                        ]
                    }
                },
                "key": "id",
                "resources": {
                    "id": "string*",
                    "account_id": "string*",
                    "name": "string*",
                    "sortable_name": "string*",
                    "last_name": "string",
                    "first_name": "string",
                    "short_name": "string*",
                    "sis_user_id": "string*",
                    "sis_import_id": "string*",
                    "integration_id": "string*",
                    "login_id": "string*",
                    "avatar_url": "string",
                    "avatar_state": "string",
                    "email": "string*",
                    "locale": "string*",
                    "last_login": "string*",
                    "time_zone": "string",
                    "bio": "string",
                    "pronouns": "string",
                    "created_at": "string*",
                    "terms_of_use": "string",
                    "skip_registration": "boolean",
                    "password": "string",
                    "send_confirmation": "boolean",
                    "force_self_registration": "boolean",
                    "authentication_provider_id": "string",
                    "enable_sis_reactivation": "boolean",
                    "destination": "string",
                    "initial_enrollment_type": "string",
                    "pairing_code": "string",
                    "confirmation_url": "string",
                    "force_validations": "boolean"
                }
            }
        }
    },
    "rest": {
        "config": {
            "baseUrl": "https://{tenant_id}/api",
            "get": {
                "pagination": {
                    "mode": "link"
                },
                "queryParameters": {
                    "per_page": 100
                }
            },
            "authentication": "aeries",
            "call_handling": "generic",
            "test_connection": {
                "url": "/v1/accounts"
            },
            "socket_timeout": 3600000,
            "retry": [
            {
                "type": "statusCode",
                "statusCode": 429,
                "retry": {
                    "type": "simple",
                    "maxCount": 12,
                    "waitTime": 5
                }
            },
            {
                "type": "statusCode",
                "statusCode": 401,
                "retry": {
                    "type": "simple",
                    "maxCount": 6,
                    "waitTime": 5
                }
            },
            {
                "type": "statusCode",
                "statusCode": 500,
                "retry": {
                    "type": "simple",
                    "maxCount": 12,
                    "waitTime": 5
                }
            },
            {
                "type": "connectionError"
            }
        ],
            "accept": "application/json",
            "headers": {
                "Authorization": "Bearer {client_secret}"
            }
        }
    },
    "connection": {
        "items": []
    }
}